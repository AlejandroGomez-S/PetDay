                     <graphic>
                        <Pane prefHeight="120.0" prefWidth="200.0">
                           <children>
                              <ImageView fx:id="imgManageRequest" fitHeight="101.0" fitWidth="200.0" pickOnBounds="true">
                                 <image>
                                    <Image url="@../Images/request.png" />
                                 </image>
                              </ImageView>
                              <Label layoutX="2.0" layoutY="98.0" prefHeight="17.0" prefWidth="196.0" text="                Manage Request" textAlignment="CENTER" />
