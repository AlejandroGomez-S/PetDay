                        <Pane prefHeight="120.0" prefWidth="200.0">
                           <children>
                              <ImageView fx:id="imgLogOut" fitHeight="104.0" fitWidth="200.0" pickOnBounds="true">
                                 <image>
                                    <Image url="@../Images/logout.png" />
                                 </image>
                              </ImageView>
                              <Label layoutX="23.0" layoutY="98.0" prefHeight="17.0" prefWidth="196.0" text="                LogOut" textAlignment="CENTER" />
                           </children>
                        </Pane>
                     </graphic>
                  </Button>
