                        <Pane prefHeight="120.0" prefWidth="200.0">
                           <children>
                              <ImageView fx:id="imgManagePets" fitHeight="100.0" fitWidth="170.0" layoutX="16.0" layoutY="-1.0" pickOnBounds="true">
                                 <image>
                                    <Image url="@../Images/huella-icono-azul.png" />
