            </VBox>
            <VBox layoutX="10.0" layoutY="10.0" prefHeight="312.0" prefWidth="256.0" spacing="30.0">
               <children>
                  <Button fx:id="btnManageAccount" mnemonicParsing="false" onAction="#actionManageAccount" prefHeight="127.0" prefWidth="218.0" text="Button">
                     <graphic>
                        <Pane prefHeight="120.0" prefWidth="200.0">
                           <children>
                              <ImageView fx:id="imgManageAccount" fitHeight="103.0" fitWidth="200.0" pickOnBounds="true">
                                 <image>
                                    <Image url="@../Images/account.png" />
                                 </image>
                              </ImageView>
                              <Label layoutX="2.0" layoutY="98.0" prefHeight="17.0" prefWidth="196.0" text="             Manage Account" textAlignment="CENTER" />
